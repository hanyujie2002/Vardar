    "@nuxt/content": "^2.13.2",
    "@nuxt/devtools": "^1.5.2",
    "@nuxt/eslint": "^0.5.7",
    "@nuxt/icon": "^1.5.2",
    "@nuxt/image": "^1.8.0",
    "@nuxtjs/color-mode": "^3.5.1",
    "@nuxtjs/i18n": "^8.5.5",
    "@nuxtjs/mdc": "^0.9.0",
    "@nuxtjs/seo": "2.0.0-rc.23",
